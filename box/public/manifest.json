{
  "name": "Scio",
  "description": "This chrome packaged app connects to an Arduino deployed with multiple sensors and communicating with the miniSDI-12 protocol.",
  "version": "1.1",
  "manifest_version": 2,
  "permissions":[
      "serial",
      "fileSystem",
      "unlimitedStorage",
      "storage",
      "http://localhost/",
      "http://fonts.googleapis.com/",
      "http://ec2-54-69-58-101.us-west-2.compute.amazonaws.com/api/*",
      "http://crypto-js.googlecode.com/"
  ],
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "icons": { "16": "16-lightbulb.png", "128": "128-lightbulb.png" }
}
